{
    "src": [
        "scripts",
        "src",
        "tests/3rdparty",
        "tests/fixtures",
        "tests/stubs",
        "tests/unit",
        "tests/bootstrap.php",
        "tests/phpstan-conditional.php",
        "tests/test-sli",
        "tools/apigen",
        ".php-cs-fixer.dist.php"
    ],
    "includeIfPhp": "/(\\/sli|\\.stub)$/",
    "enable": [
        "preserve-one-line",
        "semi-strict-expressions",
        "strict-lists"
    ],
    "operatorsFirst": true,
    "heredocIndent": "none"
}
